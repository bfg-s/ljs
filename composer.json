{
    "name": "lar/ljs",
    "description": "Laravel Application JS core",
    "type": "lar-library",
    "keywords": [
        "laravel",
        "application",
        "js",
        "core"
    ],
    "homepage": "https://github.com/bfg-s/ljs",
    "license": "MIT",
    "authors": [
        {
            "name": "bfg",
            "email": "bfg.script@gmail.com"
        }
    ],
    "prefer-stable": true,
    "require": {
        "php": ">=7.1.0",
        "ext-pdo": "*",
        "ext-json": "*",
        "ext-curl": "*"
    },
    "require-dev": {},
    "autoload": {
        "psr-4": {
            "Lar\\LJS\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lar\\LJS\\ServiceProvider"
            ],
            "aliases": {
                "LJS": "Lar\\LJS\\Facade"
            }
        }
    }
}
